<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:lottie="clr-namespace:Lottie.Forms;assembly=Lottie.Forms"
             x:Class="App4.CSharpQuizes.CSharpBasicsTwo.AwaitWrongPages.Eleven_To_Twelve_Wrong">
    <ContentPage.Content>
        <StackLayout>
            <lottie:AnimationView 
                    x:Name="checkmark"
                    IsEnabled="True"
                    Animation="2068-error-cross.json" 
                    AutoPlay="True" Loop="False"
                    HeightRequest="150"
                    WidthRequest="150"
                    VerticalOptions="FillAndExpand"
                    HorizontalOptions="FillAndExpand" />

            <lottie:AnimationView 
                    x:Name="dots"
                    IsEnabled="True"
                    Animation="1808-scaling-loader.json" 
                    AutoPlay="True" Loop="True"
                    HeightRequest="100" 
                    WidthRequest="100"
                    VerticalOptions="FillAndExpand"
                    HorizontalOptions="FillAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>